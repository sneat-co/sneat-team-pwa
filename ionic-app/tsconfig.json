{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"emitDecoratorMetadata": false,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"skipLibCheck": true,
		"lib": [
			"es2018",
			"dom"
		]
	},
	"angularCompilerOptions": {
		"enableIvy": true,
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true
	}
}
